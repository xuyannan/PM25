GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/FMDB" "${PODS_ROOT}/Headers/JSONKit" "${PODS_ROOT}/Headers/KissXML" "${PODS_ROOT}/Headers/NSLogger" "$(SDKROOT)/usr/include/libxml2"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/FMDB" -isystem "${PODS_ROOT}/Headers/JSONKit" -isystem "${PODS_ROOT}/Headers/KissXML" -isystem "${PODS_ROOT}/Headers/NSLogger"
OTHER_LDFLAGS = -ObjC -lsqlite3 -lxml2 -framework CFNetwork -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods